<h1>Listing Paquet mouses</h1>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Active</th>
      <th>X</th>
      <th>Y</th>
      <th>Pid</th>
      <th>Created at</th>
    </tr>
  </thead>

  <tbody>
    <% @paquet_mouses.each do |paquet_mouse| %>
      <tr>
        <td><%= paquet_mouse.name %></td>
        <td><%= Time.at(paquet_mouse.date.to_i) %></td>
        <td><%= paquet_mouse.active %></td>
        <td><%= paquet_mouse.x %></td>
        <td><%= paquet_mouse.y %></td>
        <td><%= paquet_mouse.pid %></td>
        <td><%= paquet_mouse.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<center><%= will_paginate @paquet_mouses, renderer: BootstrapPagination::Rails %></center>