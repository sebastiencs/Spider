<div class="row title">
	<div class="col-md-12 title">
		<h2>Spidermen</h2>
	</div>
</div>

<div class="row statsTitle">
	<div class="col-md-12"><h3>Paquets Mouses and Paquets Keys stats</h3></div>
	<div class="col-md-3"></div>
	<div class="col-md-3">
		<div class="numberKeys">
			<h3 data-toggle="tooltip" data-placement="top" title="Paquets Keys"><%= get_percent(@paquet_keys.count)%> %</h3>
		</div>
	</div>
	<div class="col-md-3">
		<div class="numberMouses">
			<h3 data-toggle="tooltip" data-placement="top" title="Paquets Mouses"><%= get_percent(@paquet_mouses.count)%> %</h3>
		</div>
	</div>
	<div class="col-md-3"></div>
</div>

<div class="row stats">
	<div class="statsNumber" paquetsKeys="<%= @paquet_keys.count.to_s.chomp(".0") %>" paquetsMouses="<%= @paquet_mouses.count.to_s.chomp(".0") %>"></div>
	<div class="col-md-12">
		<canvas id="statsPaquetAll" width="380" height="380"></canvas>
	</div>
</div>

<div class="row paquetsMouses">
	<div class="statsPaquetsMouses" none="<%= @paquet_mouses_none.count.to_s.chomp(".0") %>" left="<%= @paquet_mouses_left.count.to_s.chomp(".0") %>" right="<%= @paquet_mouses_right.count.to_s.chomp(".0") %>" middle="<%= @paquet_mouses_middle.count.to_s.chomp(".0") %>" X1Button="<%= @paquet_mouses_x1.count.to_s.chomp(".0") %>" X2Button="<%= @paquet_mouses_x2.count.to_s.chomp(".0") %>"></div>
	<div class="col-md-12"><h3>Paquets Mouses stats</h3></div>
	<div class="col-md-12">
		<canvas id="statsPaquetMouses" width="380" height="380"></canvas>
	</div>
</div>
